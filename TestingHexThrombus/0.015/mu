FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.015";
    object      mu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.004;

boundaryField
{
    INLET
    {
        type            fixedValue;
        value           uniform 0.004;
    }
    OUTLET
    {
        type            zeroGradient;
    }
    WALL
    {
        type            zeroGradient;
    }
    PATCH
    {
        type            zeroGradient;
    }
}


