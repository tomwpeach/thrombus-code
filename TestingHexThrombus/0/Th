FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Th;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
INLET
{
       type            fixedValue; 
       value           uniform 0;
}
OUTLET
{
       type            zeroGradient;
}
PATCH
{
       type            fixedValue;
       value           uniform 5e-10;
}
WALL
{
        type            zeroGradient;
}
}
